{
	"name": "pure-email-auth",
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"prepare": "husky install",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --ignore-path .gitignore --check **/*.{ts,js,json,yaml,yml,svelte,html,css} && eslint .",
		"format": "prettier --ignore-path .gitignore --write **/*.{ts,js,json,yaml,yml,svelte,html,css}",
		"changeset": "changeset",
		"upload": "CF_PAGES=1 pnpm build && wrangler pages publish --project-name pure-email-auth .svelte-kit/cloudflare",
		"tail": "wrangler pages deployment tail --project-name pure-email-auth"
	},
	"devDependencies": {
		"@changesets/changelog-github": "^0.4.8",
		"@changesets/cli": "^2.26.1",
		"@cloudflare/workers-types": "^4.20230518.0",
		"@playwright/test": "^1.34.3",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-cloudflare": "^2.3.0",
		"@sveltejs/kit": "^1.20.0",
		"@tailwindcss/typography": "^0.5.9",
		"@trivago/prettier-plugin-sort-imports": "^4.1.1",
		"@tsndr/cloudflare-worker-jwt": "^2.2.1",
		"@types/debug": "^4.1.8",
		"@typescript-eslint/eslint-plugin": "^5.59.8",
		"@typescript-eslint/parser": "^5.59.8",
		"autoprefixer": "^10.4.14",
		"daisyui": "^3.0.20",
		"debug": "^4.3.4",
		"drizzle-kit": "^0.18.1",
		"drizzle-orm": "^0.26.2",
		"eslint": "^8.41.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^8.0.3",
		"kysely": "^0.25.0",
		"kysely-d1": "^0.3.0",
		"lint-staged": "^13.2.2",
		"postcss": "^8.4.24",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"prettier-plugin-tailwindcss": "^0.3.0",
		"prisma": "^4.15.0",
		"prisma-kysely": "^1.4.2",
		"svelte": "^3.59.1",
		"svelte-check": "^3.4.3",
		"svelte-i18n": "^3.6.0",
		"sveltekit-db": "^0.1.1",
		"sveltekit-jwt": "^0.1.0",
		"sveltekit-superforms": "1.0.0-rc.3",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.5.2",
		"typescript": "^5.0.4",
		"vite": "^4.3.9",
		"vitest": "^0.31.1",
		"zod": "^3.21.4"
	},
	"lint-staged": {
		"*.{ts,js,json,yaml,yml,svelte,html,css}": [
			"prettier --write"
		]
	},
	"packageManager": "pnpm@8.6.0"
}
